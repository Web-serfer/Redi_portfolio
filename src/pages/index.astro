---
import Layout from "../layouts/Layout.astro";
import Header from "../components/header/Header.astro";
import Navbar from "../components/nav/Navbar.astro";
import About from "../components/about/About.astro";
import Experience from "../components/experience/Experience.astro";
import Services from "../components/services/Services.astro";
import Portfolio from "../components/portfolio/Portfolio.astro";
import Testimonials from "../components/testimonials/Testimonials.jsx";
import Footer from "../components/footer/Footer.astro";
import Contact from "../components/contact/Contact.astro";


const title = "Разработка сайтов в Сургуте";
---

<Layout title={title}>
  <Header />
  <Navbar />
  <About />
  <Experience />
  <Services />
  <Portfolio />
  <Testimonials client:visible />
  <Contact />
  <Footer />
</Layout>
